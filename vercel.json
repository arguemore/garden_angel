{
    "version": 2,
    "builds": [
      {
        "src": "studio/package.json",
        "use": "@vercel/static-build",
        "config": {
          "root": true,
          "distDir": "dist"
        }
      },
      {
        "src": "svelte.config.js",
        "use": "@sveltejs/kit"
      }
    ],
    "rewrites": [
      {
        "source": "/studio/(.*)",
        "destination": "/studio/$1"
      }
    ]
  }