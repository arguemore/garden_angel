{
    "version": 2,
    "builds": [
      {
        "src": "studio/package.json",
        "use": "@vercel/node",
      },
      {
        "src": "svelte.config.js",
        "use": "@sveltejs/kit"
      }
    ],
    "rewrites": [
      {
        "source": "/studio/(.*)",
        "destination": "/studio/$1"
      }
    ]
  }